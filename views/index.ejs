                <circle cx="53" cy="161" r="9" fill="#333467"/>
                <circle cx="53" cy="199" r="9" fill="#333467"/>
            </svg>                
        </section>
        <section class="new-collection-contain">
            <h3>Nieuwe Stekjes</h3>
            <% stekjes.forEach(stekje => { %>
            <ul>
                <li>
                    <img src="images/PlantSwap-logo-bruin.png" width="200" height="200">
                    <h4> <%= stekje.naam %> </h4>
                    <p> <%= stekje.aanmelddatum %> </p>
                </li>
            </ul>
            <% }) %>

            <svg width="87" height="539" viewBox="0 0 87 539" fill="none" xmlns="http://www.w3.org/2000/svg">
